<view class="charts-wrapper"><block wx:if="{{userInfo}}"><view class="user-info"><block wx:if="{{userInfo.avator}}"><image class="avator" src="{{userInfo.avator}}" mode="aspectFit"></image></block><view class="info"><block wx:if="{{userInfo.userName}}"><view class="uname">{{userInfo.userName}}</view></block><block wx:if="{{!loginState}}"><navigator class="to-login" url="../auth/login/login">登录/注册</navigator></block><block wx:if="{{userInfo.phone}}"><view class="phone">{{userInfo.phone}}</view></block></view></view></block><block wx:if="{{loginState}}"><button type="primary" data-event-opts="{{[['tap',[['logout',['$event']]]]]}}" bindtap="__e">退出登录</button></block><view class="charts-box"><box-title vue-id="31fc4540-1" title="柱状图" bind:__l="__l"></box-title><qiun-data-charts vue-id="31fc4540-2" type="column" chartData="{{chartData}}" echartsH5="{{true}}" echartsApp="{{true}}" background="#fff" bind:__l="__l"></qiun-data-charts></view><view class="charts-box"><box-title vue-id="31fc4540-3" title="折线图" bind:__l="__l"></box-title><qiun-data-charts vue-id="31fc4540-4" type="line" chartData="{{chartData}}" echartsH5="{{true}}" echartsApp="{{true}}" background="#fff" bind:__l="__l"></qiun-data-charts></view><view class="charts-box"><box-title vue-id="31fc4540-5" title="词云" bind:__l="__l"></box-title><qiun-data-charts vue-id="31fc4540-6" type="word" chartData="{{wordCloudChartData}}" bind:__l="__l"></qiun-data-charts></view></view>